---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
  <title>Buddycloud</title>
  <link href="http://buddycloud.com/feed.atom" rel="self"/>
  <link href="http://buddycloud.com/"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://buddycloud.com/</id>
  <author>
    <name>Buddycloud</name>
    <email>contact@buddycloud.com</email>
  </author>
  {% for post in site.posts limit:10 %}
  <entry>
    <title>{{ post.title | xml_escape }}</title>
    <link href="http://buddycloud.com{{ post.url }}"/>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <id>http://buddycloud.com{{ post.id }}</id>
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
  {% endfor %}
</feed>